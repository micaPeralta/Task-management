		
		<tr id="<%= task_temporary.id%>">
				<td >
					<% case task_temporary.priority %>
					<%  when "Alta" then %>
					     <span class="chip red " data-badge-caption="" >
					     	<i class="fa fa-star" aria-hidden="true"></i>
					      </span>
					<%  when "Media"  then  %>
						 <span class="chip orange" data-badge-caption="" >
						 	<i class="fa fa-star-half-o" aria-hidden="true"></i>
						 </span>
					<% when "Baja"  then  %>
					     <span class="chip gren" data-badge-caption="" >
					     	<i class="fa fa-star-o" aria-hidden="true"></i>
					      </span>
				 	<% end %> 
				</td>
				<td width="80%">

					<div>
						<div id="state-<%=task_temporary.id%>" class="inline" >
							
							<% case task_temporary.state %>
								<%  when "Hecha" then %>
								     <span class="chip tooltipped" data-position="bottom" data-delay="50" data-tooltip=<%= t('made')%>"  data-badge-caption="" > 
								     	<i class="fa fa-check" aria-hidden="true"></i>

								     </span>
								<%  when "Pendiente"  then  %>
									 <span class="chip tooltipped" data-position="bottom" data-delay="50" data-tooltip=<%= t('pending')%>
									  data-badge-caption="" >
										<i class="fa fa-thumb-tack" aria-hidden="true"></i>

									</span>
								<%  when "Expirada"  then  %>
									 <span class="chip tooltipped" data-position="bottom" data-delay="50" data-tooltip="Expirada" data-badge-caption="">
										<i class="fa fa-exclamation-triangle" aria-hidden="true"></i>

									</span>
							 <% end %>

						</div>

					  <%=  best_in_place  [@list,task_temporary], :description, :as => :textarea ,:inner_class => 'materialize-textarea'%> </div>
					<div class="footer-div">
												
						 <strong ><%= t('date-begin')%>:</strong> <%= task_temporary.date_begin %>
					
						 <strong ><%= t('date-end')%>: </strong> <%= task_temporary.date_end %>		    
			    
			 
					</div>
				
				</td>

				<td width="100px">

						<%= link_to  "/finish/task/#{task_temporary.id}", :remote=> true , method: :patch,data: { confirm: t('confirm')} do %>
							<i class="fa fa-check-square-o fa-lg" aria-hidden='true '></i>
						<%end %>

						<a href= "<%= edit_list_task_temporary_path(@list,task_temporary)%>" ><i class="material-icons">edit</i></a>
						 <%= link_to list_task_temporary_path(@list,task_temporary), method: :delete, data: { confirm: t('confirm') },:remote => true  do %>
				 		<i class="material-icons">delete</i>
						<% end %>

				</td>
				
		</tr>       
          
			   

			 

			

      