	<div class="row " style="margin-bottom: 0px">
        <div class="col s12" id="<%= task.id%>">
          <div class="card grey lighten-3">
            <div class="card-content ">
             <p> <%= task.description %><p>
             <strong> Tipo de Tarea:</strong> <%= task.type %> 
			  <strong> Estado: </strong> <%= task.state.capitalize%>
			  <strong> Prioridad: </strong> <%= task.priority.capitalize %>
             
            </div>
            <div class="card-action">
               <a onclick="showTask(<%=task.id%>)">
					<i class="material-icons">edit</i>
			   </a>

			 
						
			  <%= link_to task_path(task), method: :delete, data: { confirm: 'Are you sure?' },:remote => true  do %>
			 	<i class="material-icons">delete</i>
				<% end %>

            </div>
          </div>
        </div>
      </div>
		


						

						 	

						 	
						 	
						 

						
				